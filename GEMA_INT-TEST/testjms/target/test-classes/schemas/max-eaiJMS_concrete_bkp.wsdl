                    <xsd:documentation>E1-Interface MAX/ICE - Work Data</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="society">
                            <xsd:complexType>
                                <xsd:attribute name="cisacSocietyCode" use="required" type="st:StringMax3"/>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="interestedParties">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="interestedParty" maxOccurs="unbounded">
                                        <xsd:complexType>
                                            <xsd:attribute name="firstName" type="st:StringMax45"/>
                                            <xsd:attribute name="ipNameNumber" type="st:StringIntMax11"/>
                                            <xsd:attribute name="lastName" use="required" type="st:StringMax90"/>
                                            <xsd:attribute name="roleCode" use="required" type="ct:InterestedPartyRoleCodeType"/>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="workTitles">
                            <xsd:complexType>
                                <xsd:sequence maxOccurs="3">
                                    <xsd:element name="workTitle">
                                        <xsd:complexType>
                                            <xsd:attribute name="title" use="required" type="st:StringMax90"/>
                                            <xsd:attribute name="type" use="required" type="ct:WorkTitleType"/>
                                        </xsd:complexType>
                                    </xsd:element>
                                </xsd:sequence>
