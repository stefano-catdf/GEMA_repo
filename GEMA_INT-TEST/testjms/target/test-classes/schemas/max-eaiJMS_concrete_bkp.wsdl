            </xsd:simpleType>
            <xsd:complexType name="Identifier">
                <xsd:attribute name="publisher" type="xsd:string"/>
                <xsd:attribute name="type" use="required" type="ns4:IdentifierType"/>
                <xsd:attribute name="value" use="required" type="xsd:string"/>
            </xsd:complexType>
            <xsd:complexType name="IdentifierKeyType">
                <xsd:attribute name="contentOwner" type="xsd:string"/>
                <xsd:attribute name="type" type="ns4:IdentifierType"/>
                <xsd:attribute name="value" type="xsd:string"/>
            </xsd:complexType>
            <xsd:complexType name="IdentifierMappingType">
                <xsd:sequence>
                    <xsd:element name="key" type="ns4:IdentifierKeyType"/>
                    <xsd:element name="value" type="ns4:IdentifierValueType" minOccurs="0"/>
                </xsd:sequence>
                <xsd:attribute name="dateOfChange" type="xsd:dateTime"/>
            </xsd:complexType>
            <xsd:simpleType name="IdentifierType">
                <xsd:annotation>
                    <xsd:documentation>defines the supported types of identifiers
			</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="ISWC"/>
                    <xsd:enumeration value="ISRC"/>
                    <xsd:enumeration value="DSPWC"/>
                    <xsd:enumeration value="NWC"/>
                    <xsd:enumeration value="PWC"/>
                    <xsd:enumeration value="MC"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:complexType name="IdentifierValueType">
                <xsd:sequence>
                    <xsd:element name="workKeys">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="workKey" type="ns4:WorkKeyType" maxOccurs="unbounded"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="IdentifiersType">
                <xsd:sequence>
                    <xsd:element name="identifier" type="ns4:Identifier" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="IndexUpdateType">
                <xsd:attribute name="action" type="ns4:UpdateActionType"/>
                <xsd:attribute name="trigger" type="ns4:UpdateTriggerType"/>
            </xsd:complexType>
            <xsd:complexType name="InterestedPartiesType">
                <xsd:choice maxOccurs="unbounded">
                    <xsd:element name="artist" type="ns4:Artist"/>
                    <xsd:element name="creator" type="ns4:Creator"/>
                </xsd:choice>
            </xsd:complexType>
            <xsd:simpleType name="InterestedPartyRoleCodeType">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="ADAPTOR"/>
                    <xsd:enumeration value="ARRANGER"/>
                    <xsd:enumeration value="ARTIST"/>
                    <xsd:enumeration value="AUTHOR"/>
                    <xsd:enumeration value="COMPOSER"/>
                    <xsd:enumeration value="COMPOSER_AUTHOR"/>
                    <xsd:enumeration value="PUBLISHER"/>
                    <xsd:enumeration value="SUB_ARRANGER"/>
                    <xsd:enumeration value="SUB_AUTHOR"/>
                    <xsd:enumeration value="TRANSLATOR"/>
                    <xsd:enumeration value="WRITER"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="InterestedPartyType">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="ARTIST"/>
                    <xsd:enumeration value="CREATOR"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="InternationalClassIdentType">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="ISRC"/>
                    <xsd:enumeration value="ISWC"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="InternationalIdentType">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="ARCHIVED"/>
                    <xsd:enumeration value="PREFERRED"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="KeyType">
                <xsd:annotation>
                    <xsd:documentation>defines the supported work key types
			</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="ATC"/>
                    <xsd:enumeration value="STIM-SWC"/>
                    <xsd:enumeration value="SWC"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="MatchcodeClassIdentType">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="MC"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:complexType name="MetaType" mixed="true">
                <xsd:sequence>
                    <xsd:element name="packageDescription">
                        <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                                <xsd:whiteSpace value="collapse"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>
                    <xsd:element name="quantity" type="xsd:unsignedLong"/>
                    <xsd:element name="quantityAutomaticallyProcessed" type="xsd:unsignedLong" minOccurs="0"/>
                    <xsd:element name="quantityExported" type="xsd:unsignedLong" minOccurs="0"/>
                    <xsd:element name="contentOwner" type="xsd:string"/>
                    <xsd:element name="priority" type="ns4:PriorityType"/>
                    <xsd:element name="deadline" type="xsd:dateTime" minOccurs="0"/>
                    <xsd:element name="licensee" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="MetricComponent">
                <xsd:attribute name="lvr" use="required" type="xsd:float"/>
                <xsd:attribute name="type" use="required" type="ns4:MetricComponentType"/>
            </xsd:complexType>
            <xsd:simpleType name="MetricComponentType">
                <xsd:annotation>
                    <xsd:documentation>defines type of notation for a work
			</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="TITLE"/>
                    <xsd:enumeration value="ARTIST"/>
                    <xsd:enumeration value="CREATOR"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:complexType name="MetricComponentsType">
                <xsd:sequence>
                    <xsd:element name="metricComponent" type="ns4:MetricComponent" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:simpleType name="MetricQaReasonType">
                <xsd:annotation>
                    <xsd:documentation>defines the reason why metric QA has accepted or
				not
			</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="LOW_METRIC"/>
                    <xsd:enumeration value="HIGH_METRIC"/>
                    <xsd:enumeration value="MANUAL_ASSIGNMENT"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:complexType name="MetricQaResultType">
                <xsd:sequence>
                    <xsd:element name="evaluation" type="ns4:EvaluationType"/>
                </xsd:sequence>
                <xsd:attribute name="reason" use="required" type="ns4:MetricQaReasonType"/>
                <xsd:attribute name="result" use="required" type="ns4:QaResultType"/>
            </xsd:complexType>
            <xsd:complexType name="NotationBaseType">
                <xsd:attribute name="source" type="ns4:DocumentationSystemType"/>
                <xsd:attribute name="value" use="required" type="xsd:string"/>
            </xsd:complexType>
            <xsd:complexType name="NotationPart">
                <xsd:attribute name="type" use="required" type="ns4:PartType"/>
                <xsd:attribute name="value" use="required" type="xsd:string"/>
            </xsd:complexType>
            <xsd:complexType name="NotationWithPartsBaseType">
                <xsd:sequence>
                    <xsd:element name="part" type="ns4:NotationPart" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute name="source" type="ns4:DocumentationSystemType"/>
            </xsd:complexType>
            <xsd:complexType name="NotationsType">
                <xsd:sequence>
                    <xsd:element name="trackTitles" type="ns4:TrackTitlesType"/>
                    <xsd:element name="interestedParties" type="ns4:InterestedPartiesType" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:simpleType name="OldPartType">
                <xsd:annotation>
                    <xsd:documentation>defines the supported notation parts
			</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="FIRST_NAME"/>
                    <xsd:enumeration value="LAST_NAME"/>
                    <xsd:enumeration value="FULL_NAME"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:complexType name="OriginalTrackTitle">
                <xsd:complexContent>
                    <xsd:extension base="ns4:NotationBaseType">
                        <xsd:attribute name="subType" use="required" type="ns4:OriginalTrackTitleType"/>
                    </xsd:extension>
                </xsd:complexContent>
            </xsd:complexType>
            <xsd:simpleType name="OriginalTrackTitleType">
                <xsd:annotation>
                    <xsd:documentation>defines the supported types for original track
				title notations
			</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="UNKNOWN"/>
                    <xsd:enumeration value="ORIGINAL_TITLE"/>
                    <xsd:enumeration value="ORIGINAL_TITLE_TRANSLATED"/>
                    <xsd:enumeration value="ORIGINAL_TITLE_WITH_NATIONAL_CHARACTERS"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:complexType name="OtherQaResultType">
                <xsd:attribute name="result" use="required" type="ns4:QaResultType"/>
            </xsd:complexType>
            <xsd:simpleType name="PartType">
                <xsd:annotation>
                    <xsd:documentation>An enumeration for identifing the part of the notation</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="FULL_NAME"/>
                    <xsd:enumeration value="PART_NAME"/>
                    <xsd:enumeration value="FIRST_NAME"/>
                    <xsd:enumeration value="SECOND_FIRST_NAME"/>
                    <xsd:enumeration value="LAST_NAME"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:complexType name="PriceType">
                <xsd:simpleContent>
                    <xsd:extension base="xsd:decimal">
                        <xsd:attribute name="currency" use="required" type="ns4:CurrencyType"/>
                    </xsd:extension>
                </xsd:simpleContent>
            </xsd:complexType>
            <xsd:simpleType name="PriorityType">
                <xsd:annotation>
                    <xsd:documentation>The supported priority types. LOW to HIGH should
				be
				used for regular requests
			</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="CRITICAL"/>
                    <xsd:enumeration value="URGENT"/>
                    <xsd:enumeration value="HIGH"/>
                    <xsd:enumeration value="MEDIUM"/>
                    <xsd:enumeration value="LOW"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="PublisherType">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="OV"/>
                    <xsd:enumeration value="SV"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="QaResultType">
                <xsd:annotation>
                    <xsd:documentation>defines the kind of results the qa can delivered
			</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="REJECTED"/>
                    <xsd:enumeration value="NON_REJECTED"/>
                    <xsd:enumeration value="ACCEPTED"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:complexType name="QaResultsType">
                <xsd:sequence>
                    <xsd:element name="metricQaResult" type="ns4:MetricQaResultType" minOccurs="0"/>
                    <xsd:element name="exclusionQaResult" type="ns4:OtherQaResultType" minOccurs="0"/>
                    <xsd:element name="collectionQaResult" type="ns4:CollectionQaResultType" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:simpleType name="QaType">
                <xsd:annotation>
                    <xsd:documentation>defines the types of QA's</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="METRIC"/>
                    <xsd:enumeration value="EXCLUSION"/>
                    <xsd:enumeration value="COLLECTION"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="RegistrationStatusType">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="FREI"/>
                    <xsd:enumeration value="AUSL"/>
                    <xsd:enumeration value="NM-U"/>
                    <xsd:enumeration value="NM-E"/>
                    <xsd:enumeration value="GANZ"/>
                    <xsd:enumeration value="NM"/>
                    <xsd:enumeration value="NM-ON"/>
                    <xsd:enumeration value="NM-FS"/>
                    <xsd:enumeration value="NM-TT"/>
                    <xsd:enumeration value="STRIT"/>
                    <xsd:enumeration value="INFO"/>
                    <xsd:enumeration value="NM-R"/>
                    <xsd:enumeration value="K-AWA"/>
                    <xsd:enumeration value="INDEX"/>
                    <xsd:enumeration value="WB/RB"/>
                    <xsd:enumeration value="GAM1"/>
                    <xsd:enumeration value="UNKLA"/>
                    <xsd:enumeration value="NM-BT"/>
                    <xsd:enumeration value="VORL"/>
                    <xsd:enumeration value="PAI"/>
                    <xsd:enumeration value="MIGR"/>
                    <xsd:enumeration value="V-AV"/>
                    <xsd:enumeration value="K-GK"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="RequestType">
                <xsd:annotation>
                    <xsd:documentation>defines the delivery system's request
			</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="FULL"/>
                    <xsd:enumeration value="IDENTIFIER"/>
                    <xsd:enumeration value="TEXT"/>
                    <xsd:enumeration value="MANUAL"/>
                    <xsd:enumeration value="AUTOMATIC"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:complexType name="SearchDataType">
                <xsd:sequence>
                    <xsd:element name="identifiers" type="ns4:IdentifiersType" minOccurs="0"/>
                    <xsd:element name="notations" type="ns4:NotationsType"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="SearchHitType">
                <xsd:sequence>
                    <xsd:element name="qaResults" type="ns4:QaResultsType"/>
                </xsd:sequence>
                <xsd:attribute name="iwk" use="required" type="ns4:IceWorkKeyType"/>
                <xsd:attribute name="qa" use="required" type="ns4:QaResultType"/>
            </xsd:complexType>
            <xsd:complexType name="SearchHitsType">
                <xsd:sequence>
                    <xsd:element name="searchHit" type="ns4:SearchHitType" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="SearchResultType">
                <xsd:sequence>
                    <xsd:element name="searchHits" type="ns4:SearchHitsType"/>
                </xsd:sequence>
                <xsd:attribute name="summary" use="required" type="ns4:SummaryType"/>
                <xsd:attribute name="type" use="required" type="ns4:SearchType"/>
            </xsd:complexType>
            <xsd:complexType name="SearchResultsType">
                <xsd:sequence>
                    <xsd:element name="result" type="ns4:SearchResultType" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute name="lastProcess" use="required" type="ns4:SearchType"/>
                <xsd:attribute name="success" use="required" type="xsd:boolean"/>
            </xsd:complexType>
            <xsd:simpleType name="SearchType">
                <xsd:annotation>
                    <xsd:documentation>defines the supported search types
			</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="IDENTIFIER"/>
                    <xsd:enumeration value="TEXT"/>
                    <xsd:enumeration value="MANUAL"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="SocietyType">
                <xsd:annotation>
                    <xsd:documentation>defines the supported societies
			</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="GEMA"/>
                    <xsd:enumeration value="PRS"/>
                    <xsd:enumeration value="STIM"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="SourceNameType">
                <xsd:annotation>
                    <xsd:documentation>Owner Type</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="DIDAS"/>
                    <xsd:enumeration value="TONIS"/>
                    <xsd:enumeration value="IPI"/>
                    <xsd:enumeration value="ICE"/>
                    <xsd:enumeration value=""/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="SummaryType">
                <xsd:annotation>
                    <xsd:documentation>defines the kind of summary</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="NO_SEARCH_POSSIBLE"/>
                    <xsd:enumeration value="NO_SEARCH_HITS"/>
                    <xsd:enumeration value="NO_ACCEPTED_SEARCH_HITS"/>
                    <xsd:enumeration value="SUCCESSFUL_ASSIGNMENT"/>
                    <xsd:enumeration value="AMBIGUOUS_SEARCH_HITS"/>
                    <xsd:enumeration value="TOO_MANY_SEARCH_HITS"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:complexType name="TrackTitlesType">
                <xsd:sequence>
                    <xsd:element name="originalTrackTitle" type="ns4:OriginalTrackTitle" maxOccurs="unbounded"/>
                    <xsd:element name="alternativeTrackTitle" type="ns4:AlternativeTrackTitle" minOccurs="0" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:simpleType name="TriggerType">
                <xsd:annotation>
                    <xsd:documentation>triggering system Type</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="DIDAS"/>
                    <xsd:enumeration value="ICE"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="UpdateActionType">
                <xsd:annotation>
                    <xsd:documentation>defines the supported update actions
			</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="SUPPLY"/>
                    <xsd:enumeration value="DELETE"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="UpdateTriggerType">
                <xsd:annotation>
                    <xsd:documentation>defines the supported update triggers
			</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="ICE"/>
                    <xsd:enumeration value="MAX"/>
                    <xsd:enumeration value="PRS"/>
                    <xsd:enumeration value="STIM"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:complexType name="UsageDataType" mixed="true">
                <xsd:sequence>
                    <xsd:element name="quantity" type="xsd:integer"/>
                    <xsd:element name="price" type="ns4:PriceType"/>
                    <xsd:element name="duration" type="xsd:duration" minOccurs="0"/>
                    <xsd:element name="externalLinkID" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="Work">
                <xsd:sequence minOccurs="0">
                    <xsd:element name="additionalKeys">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="key" type="ns4:WorkKeyType" maxOccurs="unbounded"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="notations" type="ns4:NotationsType" maxOccurs="unbounded"/>
                    <xsd:element name="details" type="ns4:WorkDetailsType"/>
                </xsd:sequence>
                <xsd:attribute name="dateOfChange" type="xsd:dateTime"/>
                <xsd:attribute name="iwk" type="xsd:string"/>
            </xsd:complexType>
            <xsd:complexType name="WorkDetailsType">
                <xsd:sequence>
                    <xsd:element name="iceStatus" type="xsd:unsignedInt"/>
                    <xsd:element name="registrationStatus" type="xsd:string"/>
                    <xsd:element name="distributability" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="WorkKeyType">
                <xsd:attribute name="type" use="required" type="ns4:KeyType"/>
                <xsd:attribute name="value" use="required" type="xsd:string"/>
            </xsd:complexType>
            <xsd:simpleType name="WorkTitleSubType">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="ORIGINAL_TITLE"/>
                    <xsd:enumeration value="ALTERNATE_TITLE"/>
                    <xsd:enumeration value="FORMAL_TITLE"/>
                    <xsd:enumeration value="RESTRICTED_TITLE"/>
                    <xsd:enumeration value="FIRST_LINE_OF_TEXT"/>
                    <xsd:enumeration value="ORIGINAL_TITLE_TRANSLATED"/>
                    <xsd:enumeration value="SPECIAL_TITLE"/>
                    <xsd:enumeration value="PART_TITLE"/>
                    <xsd:enumeration value="INCORRECT_TITLE"/>
                    <xsd:enumeration value="ORIGINAL_TITLE_WITH_NATIONAL_CHARACTERS"/>
                    <xsd:enumeration value="ALTERNATE_TITLE_WITH_NATIONAL_CHARACTERS"/>
                    <xsd:enumeration value="UK_NOTATION_DISCOGRAPHY_TITLE"/>
                    <xsd:enumeration value="DURATION_TITLE"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="WorkTitleType">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="ORIGINAL_TITLE"/>
                    <xsd:enumeration value="ALTERNATE_TITLE"/>
                </xsd:restriction>
            </xsd:simpleType>
            <xsd:simpleType name="WorkType">
                <xsd:annotation>
                    <xsd:documentation>Owner Type</xsd:documentation>
                </xsd:annotation>
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="WORK"/>
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:schema>
