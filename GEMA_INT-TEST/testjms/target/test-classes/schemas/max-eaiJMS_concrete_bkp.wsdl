                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="societyKeys" minOccurs="0">
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="key" maxOccurs="unbounded">
                                                                <xsd:complexType>
                                                                    <xsd:simpleContent>
                                                                        <xsd:extension base="xsd:string">
                                                                            <xsd:attribute name="cisac" use="required" type="xsd:string"/>
                                                                            <xsd:attribute name="society" use="required" type="ct:SocietyType"/>
                                                                        </xsd:extension>
                                                                    </xsd:simpleContent>
                                                                </xsd:complexType>
                                                            </xsd:element>
                                                        </xsd:sequence>
                                                    </xsd:complexType>
                                                </xsd:element>
                                                <xsd:element name="interestedParties" minOccurs="0">
                                                    <xsd:complexType>
                                                        <xsd:sequence>
                                                            <xsd:element name="interestedParty" maxOccurs="unbounded">
                                                                <xsd:complexType>
                                                                    <xsd:sequence>
                                                                        <xsd:element name="part" maxOccurs="unbounded">
                                                                            <xsd:complexType>
                                                                                <xsd:simpleContent>
                                                                                    <xsd:extension base="xsd:string">
                                                                                        <xsd:attribute name="type" use="required" type="ct:PartType"/>
                                                                                    </xsd:extension>
                                                                                </xsd:simpleContent>
                                                                            </xsd:complexType>
                                                                        </xsd:element>
                                                                        <xsd:element name="ipBaseNumber" type="xsd:string" minOccurs="0"/>
                                                                        <xsd:element name="ipNameNumber" type="xsd:string" minOccurs="0"/>
                                                                        <xsd:element name="iceIpBaseKey" type="xsd:string" minOccurs="0"/>
                                                                        <xsd:element name="iceIpNameKey" type="xsd:string" minOccurs="0"/>
                                                                        <xsd:element name="societyIpNumbers" minOccurs="0">
                                                                            <xsd:complexType>
                                                                                <xsd:sequence>
                                                                                    <xsd:element name="societyIpNumber" minOccurs="0" maxOccurs="unbounded">
                                                                                        <xsd:complexType>
                                                                                            <xsd:sequence>
                                                                                                <xsd:element name="cisac" type="xsd:string"/>
                                                                                                <xsd:element name="name" type="xsd:string"/>
                                                                                                <xsd:element name="ipNumber" type="xsd:integer"/>
                                                                                            </xsd:sequence>
                                                                                        </xsd:complexType>
                                                                                    </xsd:element>
                                                                                </xsd:sequence>
                                                                            </xsd:complexType>
                                                                        </xsd:element>
                                                                    </xsd:sequence>
                                                                    <xsd:attribute name="roleCode" use="required" type="ct:InterestedPartyRoleCodeType"/>
                                                                    <xsd:attribute name="source" use="required" type="ct:SourceNameType"/>
                                                                    <xsd:attribute name="type" use="required" type="ct:InterestedPartyType"/>
                                                                </xsd:complexType>
