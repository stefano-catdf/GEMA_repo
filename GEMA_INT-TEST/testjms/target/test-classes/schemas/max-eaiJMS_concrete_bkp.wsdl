                                                        <xsd:sequence>
                                                            <xsd:element name="registrationStatus" type="ct:RegistrationStatusType" minOccurs="0"/>
                                                            <xsd:element name="distributility" type="ct:DistributilityType" minOccurs="0"/>
                                                            <xsd:element name="iceStatus" type="ct:IceStatusType"/>
                                                            <xsd:element name="iceStatusDesc" type="xsd:string"/>
                                                            <xsd:element name="comments" minOccurs="0">
                                                                <xsd:complexType>
                                                                    <xsd:sequence maxOccurs="unbounded">
                                                                        <xsd:element name="comment" type="xsd:string"/>
